    package="e.rahmanapyrr.gift_app">

    <application
        android:name=".ParseApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LogIn.LogInActivity">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LogIn.RegisterBirthdayActivity" />
        <activity android:name=".LogIn.RegisterActivity"/>
        <activity android:name=".LogIn.RegisterNameActivity"/>
        <activity android:name=".Friends.AddFriends"></activity>
        <activity android:name=".Calendar.CalendarActivity" />
        <activity android:name=".ProfileActivity"></activity>
        <activity android:name=".Friends.CurrentUserFriends" />
        <activity android:name=".Friends.DetailActivity">
        </activity>
        <activity android:name=".CurrentUserFriends" />
    </application>
</manifest>